# Docker Environment Configuration for TCMVE
# Copy this to .env and fill in your API keys

# LLM API Keys (at least one required for TCMVE to function)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
XAI_API_KEY=your_xai_api_key_here

# Database Configuration (Docker services)
DATABASE_URL=postgresql://tcmve:tcmve_password@db:5432/tcmve

# Redis Configuration (Docker service)
REDIS_URL=redis://redis:6379

# TCMVE Configuration
LOG_LEVEL=INFO
MAX_RETRIES=3
RETRY_DELAY=1.0

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000